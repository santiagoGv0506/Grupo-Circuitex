/**
 * Exclusive-or gate:
 * out = (not(a) and b) or (a and not(b))
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    
    Not(in=a, out=a1);
    Not(in=b, out=b1);
    And(a=a1, b=b, out=and1);
    And(a=a, b=b1, out=and2);
    Or(a=and1, b=and2, out=out);
    
    
    
    
}